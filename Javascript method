import org.apache.poi.ss.usermodel.*;

public class ExcelConverter {
    public static List<List<String>> readExcelData(String excelFilePath) throws IOException {
        List<List<String>> records = new ArrayList<>();
        FileInputStream inputStream = new FileInputStream(new File(excelFilePath));
        Workbook workbook = WorkbookFactory.create(inputStream);
        Sheet sheet = workbook.getSheetAt(0);

        for (Row row : sheet) {
            List<String> rowData = new ArrayList<>();
            for (Cell cell : row) {
                rowData.add(cell.toString());
            }
            records.add(rowData);
        }

        workbook.close();
        inputStream.close();

        return records;
    }

    public static List<Section> convertToStructuredObjects(List<List<String>> records) {
        // ... Convert to structured objects as described in previous responses ...
    }

    public static void main(String[] args) {
        try {
            List<List<String>> excelData = readExcelData("path_to_your_excel_file.xlsx");
            List<Section> structuredObjects = convertToStructuredObjects(excelData);

            SectionRepository sectionRepository = /* Inject your Section repository here */;
            sectionRepository.saveAll(structuredObjects);
        } catch (IOException e) {
            e.printStackTrace();
        }
    }
}
